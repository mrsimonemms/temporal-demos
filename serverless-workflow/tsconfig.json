{
  "compilerOptions": {
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "target": "es2024",
    "sourceMap": true,
    "baseUrl": "./",
    "noImplicitAny": true
  }
}
